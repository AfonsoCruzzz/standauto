generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Car {
  id          String   @id @default(cuid())
  brand       String
  model       String
  year        Int
  price       Float
  fuelType    String
  km          Int
  images      String[]
  description String?
  isAvailable Boolean  @default(true)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("cars")
}